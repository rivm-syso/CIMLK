    <cell relation="eu.esdihumboldt.cst.functions.groovy" id="C0eb9e5a0-1314-4072-a42f-4cdc2d8af817" priority="normal">
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="owMaatregel"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="ravMaatregel"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="emissiefactorPM10"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="eigenFactorToelichting"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="reductiefactorPM10"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="opmerking"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="ravFactor"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="aantalDieren"/>
            </property>
        </source>
        <source name="var">
            <property>
                <type name="veehouderij_bronnen"/>
                <child name="owFactor"/>
            </property>
        </source>
        <target name="result">
            <property>
                <type name="VeehouderijbronType" ns="http://cimlk.nl/1.0"/>
                <child name="emissiespecificaties" ns="http://cimlk.nl/1.0"/>
            </property>
        </target>
        <complexParameter name="script">
            <core:text xmlns:core="http://www.esdi-humboldt.eu/hale/core" xml:space="preserve">
_target {&#xD;
	Emissiespecificatie {&#xD;
		aantalDieren aantalDieren&#xD;
		if(ravFactor == "Exp.Stal") {&#xD;
			eigenEmissiefactorPm10 emissiefactorPM10&#xD;
		}&#xD;
		if(ravMaatregel == "Overig") {&#xD;
			eigenReductiefactorPm10 reductiefactorPM10&#xD;
		}&#xD;
		if(ravFactor == "Exp.Stal" || ravMaatregel == "Overig") {&#xD;
			eigenFactorToelichting eigenFactorToelichting&#xD;
		}&#xD;
		opmerking opmerking&#xD;
		ravFactor {&#xD;
			href '#'+ravFactor&#xD;
		}&#xD;
		if (ravMaatregel) {&#xD;
			ravMaatregel {&#xD;
				href '#'+ravMaatregel&#xD;
			}&#xD;
		}&#xD;
	}&#xD;
}
</core:text>
        </complexParameter>
        <parameter value="false" name="variablesAsInstances"/>
    </cell>
